<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Vegg">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="VEGETABLES"
        android:gravity="center"
        android:textSize="37sp"
        android:textStyle="bold"
        android:textColor="#ffff"
        android:background="#70CC74"
        />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:layout_marginLeft="0dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/tomato"
            android:layout_width="116dp"
            android:layout_height="120dp"
            app:srcCompat="@drawable/tomato" />

        <ImageView
            android:id="@+id/capcicum"
            android:layout_width="116dp"
            android:layout_height="120dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/capcicum" />
        <ImageView
            android:layout_width="116dp"
            android:layout_height="120dp"
            android:id="@+id/carrot"
            android:layout_marginLeft="10dp"
            android:src="@drawable/carrots"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="215dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Tomato"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Capcicum"
            android:layout_marginLeft="55dp"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Carrot"
            android:layout_marginLeft="50dp"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="260dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/eff"
            android:layout_width="116dp"
            android:layout_height="120dp"
            app:srcCompat="@drawable/effplant" />

        <ImageView
            android:id="@+id/cucumber"
            android:layout_width="116dp"
            android:layout_height="120dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/cucumber" />
        <ImageView
            android:layout_width="116dp"
            android:layout_height="120dp"
            android:id="@+id/onion"
            android:layout_marginLeft="10dp"
            android:src="@drawable/onion"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="370dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="brinjal"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cucumber"
            android:layout_marginLeft="55dp"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Onion"
            android:layout_marginLeft="53dp"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="410dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/potato"
            android:layout_width="116dp"
            android:layout_height="120dp"
            app:srcCompat="@drawable/potato" />

        <ImageView
            android:id="@+id/pumpkin"
            android:layout_width="116dp"
            android:layout_height="120dp"
            android:layout_marginLeft="10dp"
            android:src="@drawable/pumkin" />
        <ImageView
            android:layout_width="116dp"
            android:layout_height="120dp"
            android:id="@+id/bean"
            android:layout_marginLeft="10dp"
            android:src="@drawable/bean"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="525dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Potato"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Pumpkin"
            android:layout_marginLeft="55dp"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Beans"
            android:layout_marginLeft="63dp"
            android:textColor="#EB0A0300"
            android:textSize="20sp"
            android:textStyle="bold"/>
    </LinearLayout>


</RelativeLayout>